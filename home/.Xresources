! Colors
#include "/home/charlie/.termcolor/shell-x"

! URxvt
URxvt.termName:         rxvt-256color
URxvt.font:             xft:Liberation Mono:Regular:size=10, xft:Liberation Mono For Powerline:Regular:size=10
URxvt.iconFile:         /usr/share/icons/AwOkenDark/clear/48x48/apps/xterm-color_48x48.png
URxvt.cursorBlink:      true
URxvt.pointerBlank:     true
URxvt.scrollBar:        false
URxvt.saveLines:        10000
URxvt.scrollWithBuffer: true
URxvt.loginShell:       true

URxvt.perl-ext-common:      default,keyboard-select,url-select,clipboard,vtwheel

URxvt.keysym.M-Escape:      perl:keyboard-select:activate
URxvt.keysm.M-s:            perl:keyboard-select:search

URxvt.keysym.M-u:           perl:url-select:select_next
URxvt.url-select.button:    1
URxvt.url-select.launcher:  firefox -new-tab
URxvt.url-select.underline: true

URxvt.keysym.M-c:           perl:clipboard:copy
URxvt.keysym.M-v:           perl:clipboard:paste
URxvt.keysym.M-C-v:         perl:clipboard:paste_escaped
