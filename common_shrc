export PATH=$PATH:$HOME/.bin # User scripts/executables

# use nodenv for node
export PATH="$HOME/.nodenv/bin:$PATH"
eval "$(nodenv init -)"

export EDITOR="vim"
export BROWSER="chromium"
eval `dircolors ~/.config/dircolors-solarized/dircolors.256dark`

[[ -f ~/.aliases_sh ]] && source ~/.aliases_sh
