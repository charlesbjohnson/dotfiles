#
# These things are run when an Openbox X Session is started.
# You may place a similar script in $HOME/.config/openbox/autostart
# to run user-specific things.
#

# If you want to use GNOME config tools...
#
#if test -x /usr/lib/openbox/gnome-settings-daemon >/dev/null; then
#  /usr/lib/openbox/gnome-settings-daemon &
#elif which gnome-settings-daemon >/dev/null 2>&1; then
#  gnome-settings-daemon &
#fi

# If you want to use XFCE config tools...
#
#xfce-mcs-manager &
#
#
feh --bg-scale ~/wallpapers/current_wallpaper &
(sleep 1s && syndaemon -k -i 1 -d) &
(sleep 1s && compton --config ~/.config/compton.conf) &
(sleep 1s && tint2) &
(sleep 1s && xscreensaver -no-splash) &
(sleep 1s && dropboxd) &
(sleep 1s && xfce4-volumed) &
(sleep 1s && volumeicon) &
(sleep 1s && xfce4-clipman) &
(sleep 1s && netmon) &
(sleep 1s && redshift -l 41.0:-73.6) &
(sleep 1s && thunar --daemon) &
(sleep 1s && /usr/share/aarchup/aarchstartup.sh) &
(sleep 1s && setxkbmap -option caps:escape) &
