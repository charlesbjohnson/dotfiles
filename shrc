export PATH="$PATH:$HOME/.bin"

export PATH="$HOME/.nodenv/bin:$PATH"
eval "$(nodenv init -)"

export EDITOR="vim"
export BROWSER="chromium"

[[ -f  ~/.config/dircolors-solarized/dircolors.256dark ]] && eval `dircolors ~/.config/dircolors-solarized/dircolors.256dark`

[[ -f ~/.shrc.local ]] && source ~/.shrc.local
[[ -f ~/.aliases ]] && source ~/.aliases
