[push]
  default = current
[core]
  excludesfile = ~/.gitignore
  autocrlf = input
[commit]
  template = ~/.gitmessage
[user]
  email = mail@cbjohnson.info
  name = Charles B Johnson
[color]
  ui = true
[web]
  browser = chromium
[credential]
  helper = /usr/share/doc/git/contrib/credential/gnome-keyring/git-credential-gnome-keyring
[alias]
  amend = commit --amend
  unstage = reset HEAD --
  staged = diff --staged
