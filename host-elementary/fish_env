if [ -z (echo $TERM | grep 256color) ]
  set -x TERM xterm-256color
end

if [ ! $SHELL_COLORED ]
  if [ (ps -o comm= -p $PPID) = pantheon-termin ]
    eval sh ~/.termcolor/base16-pantheon-terminal/base16-solarized.dark.sh
  end

  eval sh ~/.termcolor/base16-shell/base16-solarized.dark.sh
  set -x SHELL_COLORED true
end
